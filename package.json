{
  "name": "rc-image-loader",
  "version": "1.0.6",
  "description": "handle render a preview image is tiny while waiting for real image loading. the real image is lazy-loaded when loaded. support server-side render for seo!",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "build:watch": "babel src --out-dir dist --watch",
    "clean": "rimraf dist",
    "lint": "eslint --config .eslintrc . --ignore-path .gitignore --fix",
    "flow": "flow",
    "check": "npm run lint && npm run flow",
    "test": "jest",
    "pre:pr": "npm run clean && npm run build && npm run lint && npm run test",
    "pre:publish": "npm run clean && npm run build && npm run check && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lamhieu-vk/rc-image-loader.git"
  },
  "keywords": [
    "react",
    "image",
    "progressive",
    "loader",
    "render",
    "lazy",
    "placeholder"
  ],
  "author": "Lam Hieu <lamhieu.vk@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow": "^0.2.3",
    "flow-bin": "^0.91.0",
    "jest": "^23.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
